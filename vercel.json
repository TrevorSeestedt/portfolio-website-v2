{
  "functions": {
    "api/spotify.js": {
      "runtime": "@vercel/node@18.x"
    },
    "api/login.js": {
      "runtime": "@vercel/node@18.x"
    },
    "api/callback.js": {
      "runtime": "@vercel/node@18.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/login",
      "destination": "/api/login"
    },
    {
      "source": "/callback",
      "destination": "/api/callback"
    }
  ]
} 